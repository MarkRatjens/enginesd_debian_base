{
	"about": {
		"title": "enginesd_debian_base",
		"explanation": "Docker Base for debian images"
	},
	"configuration": {
		"locale": "en_AU",
		"charset": "UTF-8",
		"timezone": "SystemConfig(default_timezone)"
	},
	"binding_target": {
		"locale": "^^configuration.locale^^",
		"charset": "^^configuration.charset^^",
		"timezone": "^^configuration.timezone^^"
	},
	"system_packages": {
		"adds": [
			"apt-transport-https",
			"unzip"
		],
		"removes": [
			"bootlogd",
			"dmidecode",
			"eudev",
			"gdbm-l10n",
			"iptables",
			"iputils-ping",
			"kmod",
			"krb5-locales",
			"less",
			"libip4tc0",
			"libip6tc0",
			"libiptc0",
			"libkmod2",
			"libnetfilter-conntrack3",
			"libnfnetlink0",
			"libnftnl11",
			"tasksel",
			"tasksel-data"
		]
	},
	"packing": {
    "early": [
	 "locale",
      "apt",
      "locale",
      "repos"
    ],
		"late": [
		],
		"last": [
      "cleanup"
    ]
  },
	"images": [
		{
			"type": "docker",
			"image": "debian"
		},
		{
			"type": "lxd",
			"image": "images:devuan/beowulf/cloud"
		}
	]
}
